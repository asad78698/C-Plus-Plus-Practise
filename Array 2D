#include <iostream>
using namespace std;

int main ()

{
	
int A [4][2];

for (int i=0; i<4; i++)


{
	cout<<"Enter Student "<<i<<" Age And Marks "<<endl;
	
	
for (int j=0; j<2; ++j)
				

		cin>>A[i][j];
	
}

cout<<"Age \t Marks "<<endl;


for(int i=0; i<4; ++i)
{
 
 for (int j=0; j<2; ++j)
 
 	cout<<A[i][j]<<"\t";
 	
 
 cout<<endl;
 
}



}
